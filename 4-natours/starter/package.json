{
  "name": "starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "clean-install": "npm run clean && npm install",
    "build": "npm run clean && tsc",
    "start": "ts-node server.ts",
    "dev": "concurrently \"npm run start\" \"npm run build -- --watch\"",
    "start-dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --config .prettierrc --write 'src/**/*.ts' 'dev-data/data/**/*.json'",
    "debug": "tsc --watch & nodemon --inspect-brk=9226 --ext js,html /dist/index.js",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1"
  }
}
